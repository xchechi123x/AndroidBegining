<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="navigation_bottom">
        <attr name="elevation" format="dimension" />
        <attr name="active_color" format="color" />
        <attr name="in_active_color" format="color" />
        <attr name="bg_color" format="color" />
        <attr name="animation_duration" format="integer" />
        <attr name="auto_hide_enabled" format="boolean" />
        <attr name="mode" format="enum">
            <enum name="mode_default" value="0" />
            <enum name="mode_fixed" value="1" />
            <enum name="mode_shifting" value="2" />
        </attr>
        <attr name="bg_style" format="enum">
            <enum name="background_style_default" value="0" />
            <enum name="background_style_static" value="1" />
            <enum name="background_style_ripple" value="2" />
        </attr>

    </declare-styleable>
</resources>
